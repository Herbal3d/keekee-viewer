{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (KeeKee)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-local-libremetaverse",
            "program": "${workspaceFolder}/dist/KeeKee.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {
                "DOTNET_ENVIRONMENT": "Debug",
                "ASPNETCORE_ENVIRONMENT": "Debug"
            },
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
