{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-local-libremetaverse",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-c",
                "Debug",
                "-p:LibreMetaverseLocalDir=../libremetaverse-mb/bin/net9.0"
            ],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": ["build", "-c", "Debug"],
            "problemMatcher": "$msCompile",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
