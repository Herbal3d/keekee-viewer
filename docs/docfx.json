{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "KeeKee.Renderer/KeeKee.Renderer.csproj",
            "KeeKee.Comm.LLLP/KeeKee.Comm.LLLP.csproj",
            "KeeKee.Renderer.Map/KeeKee.Renderer.Map.csproj",
            "KeeKee.World/KeeKee.World.csproj",
            "KeeKee.World.Services/KeeKee.World.Services.csproj",
            "KeeKee.Framework/KeeKee.Framework.csproj",
            "KeeKee.Comm/KeeKee.Comm.csproj",
            "KeeKee.World.LL/KeeKee.World.LL.csproj",
            "KeeKee/KeeKee.csproj",
            "KeeKee.Rest/KeeKee.Rest.csproj",
            "KeeKee.Config/KeeKee.Config.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "docfx.usage.notes"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "KeeKee-viewer",
      "_appTitle": "KeeKee-viewer",
      "_enableSearch": true,
      "pdf": true
    }
  }
}
