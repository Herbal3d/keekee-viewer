{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "outDir": "./dist/KeeKeeResources/web",
        "rootDir": "./KeeKeeResources/web",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "declaration": false,
        "sourceMap": true
    },
    "include": ["KeeKeeResources/web/**/*.ts"],
    "exclude": ["node_modules", "dist", "**/*.spec.ts"]
}
